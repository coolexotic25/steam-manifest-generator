# Production Environment Variables Template
# Copy this to .env.production for production deployment

# Discord Bot Configuration
DISCORD_TOKEN=your_production_discord_token
CLIENT_ID=your_production_discord_client_id

# Website Configuration  
WEBSITE_URL=https://your-domain.vercel.app

# Database Configuration
DATABASE_URL=your_production_database_url

# Steam API Configuration
STEAM_API_KEY=your_production_steam_api_key

# Node Environment
NODE_ENV=production