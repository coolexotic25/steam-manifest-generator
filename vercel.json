{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/server.js"
    },
    {
      "src": "/download/(.*)",
      "dest": "/server.js"
    },
    {
      "src": "/(.*)",
      "dest": "/server.js"
    }
  ],
  "env": {
    "DATABASE_URL": "@database_url",
    "DISCORD_TOKEN": "@discord_token",
    "STEAM_API_KEY": "@steam_api_key"
  },
  "functions": {
    "server.js": {
      "maxDuration": 30
    }
  }
}