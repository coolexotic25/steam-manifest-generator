{
  "name": "steam-manifest-generator",
  "version": "2.0.0",
  "description": "A Discord bot and website that generates Steam app manifests and Lua scripts with real Steam API data and Steamtools compatibility",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "deploy": "node config/deploy-commands.js",
    "dev": "next dev -p 3000",
    "bot": "nodemon index.js",
    "server": "node server.js",
    "build": "next build",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:studio": "prisma studio"
  },
  "keywords": [
    "discord",
    "bot",
    "steam",
    "manifest",
    "lua",
    "generator",
    "steamtools",
    "nextjs",
    "prisma"
  ],
  "author": "Steam Manifest Generator",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/steam-manifest-generator"
  },
  "bugs": {
    "url": "https://github.com/yourusername/steam-manifest-generator/issues"
  },
  "homepage": "https://github.com/yourusername/steam-manifest-generator#readme",
  "dependencies": {
    "@prisma/client": "^5.7.1",
    "@supabase/supabase-js": "^2.83.0",
    "axios": "^1.6.2",
    "cors": "^2.8.5",
    "discord.js": "^14.14.1",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "next": "^16.0.3",
    "prisma": "^5.7.1",
    "react": "^19.2.0",
    "react-dom": "^19.2.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "eslint": "^8.0.0",
    "nodemon": "^3.0.2",
    "prettier": "^3.0.0",
    "typescript": "^5.0.0",
    "vercel": "^48.10.4"
  },
  "engines": {
    "node": ">=16.11.0"
  }
